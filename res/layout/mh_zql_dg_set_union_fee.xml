<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:gravity="center" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/ll_feel" android:background="@drawable/mh_zql_white_shape" android:paddingLeft="@dimen/dp_20" android:paddingRight="@dimen/dp_20" android:paddingBottom="@dimen/dp_20" android:layout_width="312dp" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_13" android:layout_marginRight="@dimen/dp_13" android:layout_below="@+id/img_title">
        <TextView android:textSize="@dimen/sp_17" android:textColor="@color/white" android:gravity="center" android:layout_gravity="center_horizontal" android:id="@+id/tv_fee_title" android:background="@mipmap/mh_zql_union_tiitle" android:layout_width="186dp" android:layout_height="@dimen/dp_32" android:text="我的分佣比例" android:layout_centerHorizontal="true"/>
        <TextView android:textSize="13sp" android:textColor="#ff3b3839" android:id="@+id/tv_fee_tip" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="23dp" android:text="设置我的分佣比例（最多40%）" android:layout_below="@+id/tv_fee_title"/>
        <TextView android:textSize="15sp" android:textColor="#ff000000" android:gravity="center" android:id="@+id/tv_fee_bf" android:layout_width="wrap_content" android:layout_height="@dimen/dp_32" android:layout_marginTop="@dimen/dp_16" android:text="%" android:layout_below="@+id/tv_fee_tip" android:layout_alignParentRight="true"/>
        <EditText android:textColor="#000000" android:id="@+id/ed_fee" android:background="@drawable/mh_zql_union_ed" android:paddingLeft="10dp" android:layout_width="match_parent" android:layout_height="@dimen/dp_32" android:layout_marginTop="@dimen/dp_16" android:layout_marginRight="@dimen/dp_20" android:maxLength="2" android:layout_toLeftOf="@+id/tv_fee_bf" android:layout_below="@+id/tv_fee_tip" android:inputType="number"/>
        <TextView android:textColor="@color/white" android:gravity="center" android:id="@+id/btn_cancel" android:background="@drawable/mh_zql_union_btn" android:layout_width="110dp" android:layout_height="34dp" android:layout_marginLeft="@dimen/dp_11" android:layout_marginTop="@dimen/dp_24" android:text="取消" android:layout_below="@+id/ed_fee"/>
        <TextView android:textColor="@color/white" android:gravity="center" android:id="@+id/btn_sure" android:background="@drawable/mh_zql_union_btn" android:layout_width="110dp" android:layout_height="34dp" android:layout_marginTop="@dimen/dp_24" android:layout_marginRight="@dimen/dp_11" android:text="确定" android:layout_below="@+id/ed_fee" android:layout_alignParentRight="true"/>
    </RelativeLayout>
</LinearLayout>
