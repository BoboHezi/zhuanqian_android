<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include layout="@layout/yd_xo_common_white_header_layout" />

    <RelativeLayout
        android:id="@+id/btn_withdraw_income"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_48"
        android:layout_marginLeft="@dimen/dp_16"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/img_withdraw_income"
            android:layout_width="@dimen/dp_19"
            android:layout_height="@dimen/dp_19"
            android:src="@mipmap/yd_xo_withdraw_income_icon" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_toRightOf="@+id/img_withdraw_income"
            android:text="收入分红"
            android:textColor="@color/color_FF333333"
            android:textSize="@dimen/sp_16" />

        <ImageView
            android:layout_width="@dimen/dp_18"
            android:layout_height="@dimen/dp_18"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp_16"
            android:src="@mipmap/yd_xo_me_arrow_icon" />
    </RelativeLayout>

    <View
        style="@style/line"
        android:layout_marginLeft="@dimen/dp_16" />

    <RelativeLayout
        android:id="@+id/btn_withdraw_balance"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_48"
        android:layout_marginLeft="@dimen/dp_16"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/img_withdraw_balance"
            android:layout_width="@dimen/dp_19"
            android:layout_height="@dimen/dp_19"
            android:src="@mipmap/yd_xo_withdraw_balance_icon" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_toRightOf="@+id/img_withdraw_balance"
            android:text="任务余额"
            android:textColor="@color/color_FF333333"
            android:textSize="@dimen/sp_16" />

        <ImageView
            android:layout_width="@dimen/dp_18"
            android:layout_height="@dimen/dp_18"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp_16"
            android:src="@mipmap/yd_xo_me_arrow_icon" />
    </RelativeLayout>

    <View
        style="@style/line"
        android:layout_marginLeft="@dimen/dp_16" />

    <RelativeLayout
        android:id="@+id/btn_withdraw_margin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_48"
        android:layout_marginLeft="@dimen/dp_16"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/img_withdraw_margin"
            android:layout_width="@dimen/dp_19"
            android:layout_height="@dimen/dp_19"
            android:src="@mipmap/yd_xo_withdraw_margin_icon" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_toRightOf="@+id/img_withdraw_margin"
            android:text="保证金"
            android:textColor="@color/color_FF333333"
            android:textSize="@dimen/sp_16" />

        <ImageView
            android:layout_width="@dimen/dp_18"
            android:layout_height="@dimen/dp_18"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp_16"
            android:src="@mipmap/yd_xo_me_arrow_icon" />
    </RelativeLayout>

    <View
        style="@style/line"
        android:layout_marginLeft="@dimen/dp_16" />
</LinearLayout>
