<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent">
    <include layout="@layout/mh_zql_common_white_header_layout"/>
    <RelativeLayout android:gravity="center_vertical" android:id="@+id/btn_withdraw_income" android:layout_width="match_parent" android:layout_height="@dimen/dp_48" android:layout_marginLeft="@dimen/dp_16">
        <ImageView android:id="@+id/img_withdraw_income" android:layout_width="@dimen/dp_19" android:layout_height="@dimen/dp_19" android:src="@mipmap/mh_zql_withdraw_income_icon"/>
        <TextView android:textSize="@dimen/sp_16" android:textColor="@color/color_FF333333" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_15" android:text="收入分红" android:layout_toRightOf="@+id/img_withdraw_income"/>
        <ImageView android:layout_width="@dimen/dp_18" android:layout_height="@dimen/dp_18" android:layout_marginRight="@dimen/dp_16" android:src="@mipmap/mh_zql_me_arrow_icon" android:layout_alignParentRight="true"/>
    </RelativeLayout>
    <View android:layout_marginLeft="@dimen/dp_16" style="@style/line"/>
    <RelativeLayout android:gravity="center_vertical" android:id="@+id/btn_withdraw_balance" android:layout_width="match_parent" android:layout_height="@dimen/dp_48" android:layout_marginLeft="@dimen/dp_16">
        <ImageView android:id="@+id/img_withdraw_balance" android:layout_width="@dimen/dp_19" android:layout_height="@dimen/dp_19" android:src="@mipmap/mh_zql_withdraw_balance_icon"/>
        <TextView android:textSize="@dimen/sp_16" android:textColor="@color/color_FF333333" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_15" android:text="任务余额" android:layout_toRightOf="@+id/img_withdraw_balance"/>
        <ImageView android:layout_width="@dimen/dp_18" android:layout_height="@dimen/dp_18" android:layout_marginRight="@dimen/dp_16" android:src="@mipmap/mh_zql_me_arrow_icon" android:layout_alignParentRight="true"/>
    </RelativeLayout>
    <View android:layout_marginLeft="@dimen/dp_16" style="@style/line"/>
    <RelativeLayout android:gravity="center_vertical" android:id="@+id/btn_withdraw_margin" android:layout_width="match_parent" android:layout_height="@dimen/dp_48" android:layout_marginLeft="@dimen/dp_16">
        <ImageView android:id="@+id/img_withdraw_margin" android:layout_width="@dimen/dp_19" android:layout_height="@dimen/dp_19" android:src="@mipmap/mh_zql_withdraw_margin_icon"/>
        <TextView android:textSize="@dimen/sp_16" android:textColor="@color/color_FF333333" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_15" android:text="保证金" android:layout_toRightOf="@+id/img_withdraw_margin"/>
        <ImageView android:layout_width="@dimen/dp_18" android:layout_height="@dimen/dp_18" android:layout_marginRight="@dimen/dp_16" android:src="@mipmap/mh_zql_me_arrow_icon" android:layout_alignParentRight="true"/>
    </RelativeLayout>
    <View android:layout_marginLeft="@dimen/dp_16" style="@style/line"/>
</LinearLayout>
