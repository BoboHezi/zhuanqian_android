<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent">
    <include layout="@layout/mh_zql_common_header_layout"/>
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
        <RelativeLayout android:id="@+id/btn_phone" android:layout_width="match_parent" android:layout_height="74dp">
            <ImageView android:id="@+id/img_phone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_16" android:layout_marginRight="@dimen/dp_12" android:src="@mipmap/mh_zql_contact_mobile" android:layout_centerVertical="true"/>
            <TextView android:textSize="16sp" android:textStyle="bold" android:textColor="@color/color_FF333333" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_15" android:text="手机号" android:layout_toRightOf="@+id/img_phone"/>
            <TextView android:textSize="13sp" android:textColor="#ff67696b" android:id="@+id/tv_phone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/dp_15" android:text="" android:layout_toRightOf="@+id/img_phone" android:layout_alignParentBottom="true"/>
            <TextView android:textSize="14sp" android:textColor="@color/white" android:gravity="center" android:id="@+id/btn_open_phone" android:background="@drawable/mh_zql_task_public" android:layout_width="80dp" android:layout_height="34dp" android:layout_marginRight="@dimen/dp_16" android:text="复制拨号" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
            <TextView android:layout_toRightOf="@+id/img_phone" android:layout_alignParentBottom="true" style="@style/line"/>
        </RelativeLayout>
        <RelativeLayout android:id="@+id/btn_wx" android:layout_width="match_parent" android:layout_height="74dp">
            <ImageView android:id="@+id/img_wx" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_16" android:layout_marginRight="@dimen/dp_12" android:src="@mipmap/mh_zql_contact_wx" android:layout_centerVertical="true"/>
            <TextView android:textSize="16sp" android:textStyle="bold" android:textColor="@color/color_FF333333" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_15" android:text="客服微信" android:layout_toRightOf="@+id/img_wx"/>
            <TextView android:textSize="13sp" android:textColor="#ff67696b" android:id="@+id/tv_wx" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/dp_15" android:text="" android:layout_toRightOf="@+id/img_wx" android:layout_alignParentBottom="true"/>
            <TextView android:textSize="14sp" android:textColor="@color/white" android:gravity="center" android:id="@+id/btn_open_wx" android:background="@drawable/mh_zql_task_public" android:layout_width="80dp" android:layout_height="34dp" android:layout_marginRight="@dimen/dp_16" android:text="复制打开" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
            <TextView android:layout_toRightOf="@+id/img_wx" android:layout_alignParentBottom="true" style="@style/line"/>
        </RelativeLayout>
        <RelativeLayout android:id="@+id/btn_qq" android:layout_width="match_parent" android:layout_height="74dp">
            <ImageView android:id="@+id/img_qq" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_16" android:layout_marginRight="@dimen/dp_12" android:src="@mipmap/mh_zql_contact_qq" android:layout_centerVertical="true"/>
            <TextView android:textSize="16sp" android:textStyle="bold" android:textColor="@color/color_FF333333" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_15" android:text="客服QQ" android:layout_toRightOf="@+id/img_qq"/>
            <TextView android:textSize="13sp" android:textColor="#ff67696b" android:id="@+id/tv_qq" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/dp_15" android:text="" android:layout_toRightOf="@+id/img_qq" android:layout_alignParentBottom="true"/>
            <TextView android:textSize="14sp" android:textColor="@color/white" android:gravity="center" android:id="@+id/btn_open_qq" android:background="@drawable/mh_zql_task_public" android:layout_width="80dp" android:layout_height="34dp" android:layout_marginRight="@dimen/dp_16" android:text="复制打开" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
            <TextView android:layout_toRightOf="@+id/img_qq" android:layout_alignParentBottom="true" style="@style/line"/>
        </RelativeLayout>
    </LinearLayout>
    <TextView android:background="#f4f7fa" android:layout_width="match_parent" android:layout_height="match_parent"/>
</LinearLayout>
